{
  "name": "node-backend",
  "version": "1.0.0",
  "description": "RESTful API for Task Management application",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.ts",
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:unit": "jest --config jest.config.js --testPathPattern=test/unit",
    "test:integration": "jest --config jest.config.js --testPathPattern=test/integration",
    "test:coverage": "jest --config jest.config.js --coverage",
    "migrate": "sequelize-cli db:migrate",
    "seed": "sequelize-cli db:seed:all"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "mysql",
    "sequelize",
    "aws",
    "serverless"
  ],
  "author": "Perseverance Ngoy",
  "license": "ISC",
  "dependencies": {
    "@types/aws-lambda": "^8.10.147",
    "aws-sdk": "^2.1692.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "esbuild": "^0.25.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^6.0.1",
    "joi": "^17.9.1",
    "mysql2": "^3.2.0",
    "pino": "^8.11.0",
    "pino-http": "^8.3.3",
    "sequelize": "^6.30.0",
    "sequelize-typescript": "^2.1.5",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.15.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-promise": "^6.6.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
